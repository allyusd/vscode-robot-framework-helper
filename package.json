{
	"name": "robot-framework-helper",
	"displayName": "Robot Framework Helper",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rfhelp.replaceClipboardTextIncludeUnderscoreAndSpace"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rfhelp.replaceClipboardTextIncludeUnderscoreAndSpace",
				"title": "Hello World"
			}
		],
		"keybindings": [{
			"command": "rfhelp.replaceClipboardTextIncludeUnderscoreAndSpace",
			"key": "ctrl+shift+f12"
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
